(function(a){a.fn.dolRSSFeed=function(b){return this.each(function(){var d=a(this);var f=d.attr("rssid");if(!f&&b==undefined){return false}var c=parseInt(d.attr("rssnum")||0);var e=parseInt(d.attr("member")||0);var g=(b!=undefined)?b:site_url+"get_rss_feed.php?ID="+f+"&member="+e;d.bx_loading(true);a.getFeed({url:g,success:function(m){if(m!=undefined&&m.items){var i='<div class="rss_feed_wrapper bx-def-bc-margin">';var h,j=0;for(var n=0;n<m.items.length;n++){var o=m.items[n];var k="",p,l;if(null!=(l=o.updated.match(/(\d+)-(\d+)-(\d+)T(\d+):(\d+):(\d+)Z/))){p=new Date(l[1],l[2]-1,l[3],l[4],l[5],l[6],0);k=p.toLocaleString()}else{if(o.updated.length>0){p=new Date(o.updated.replace(/z$/i,"-00:00"));k=isNaN(p)?"":p.toLocaleString()}}h="";if(o.link.substring(0,site_url.length)!=site_url){h='target="_blank"'}i+='<hr class="bx-def-hr bx-def-margin-sec-top bx-def-margin-sec-bottom" /><div class="rss_item_wrapper"><div class="rss_item_header bx-def-font-h2"><a href="'+o.link+'" '+h+">"+o.title+'</a></div><div class="rss_item_desc">'+o.description+'</div><div class="rss_item_info bx-def-font-small bx-def-font-grayed"><span>'+k+"</span></div></div>";j++;if(j==c){break}}h="";if(m.link.substring(0,site_url.length)!=site_url){h='target="_blank"'}i+='</div><div class="rss_read_more bx-def-padding-left bx-def-padding-right"><a href="'+m.link+'" '+h+' class="rss_read_more_link">'+m.title+'</a></div><div class="clear_both"></div>';d.html(i)}}})})}})(jQuery);

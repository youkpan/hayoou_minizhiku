(function(a){a.fn.dolRetina=function(c){var b={retina_part:"-2x"};if(c){jQuery.extend(b,{retina_part:c})}if(window.devicePixelRatio>=2){this.each(function(e,g){if(!a(g).attr("src")){return}var f="";if(!a(g).attr("src-2x")){var d=new RegExp("(.+)("+b.retina_part+"\\.\\w{3,4})");if(d.test(a(g).attr("src"))){return}var f=a(g).attr("src").replace(/(.+)(\.\w{3,4})$/,"$1"+b.retina_part+"$2")}else{f=a(g).attr("src-2x")}if(!f){return}a.ajax({url:f,type:"HEAD",success:function(){a(g).attr("src",f)}})})}return this}})(jQuery);

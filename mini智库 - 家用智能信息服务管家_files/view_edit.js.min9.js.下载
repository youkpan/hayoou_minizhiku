function submitViewEditForm(eForm,iPageBlockID,sSuccessUrl){if(!eForm){return false}hideEditFormErrors(eForm);$(eForm).ajaxSubmit({success:function(sResponce){try{var aErrors=eval(sResponce)}catch(e){return false}var bHaveErrors=doShowEditErrors(aErrors,eForm);if(!bHaveErrors){loadDynamicBlock(iPageBlockID,sSuccessUrl);closeDynamicPopupBlock()}}});return false}function hideEditFormErrors(a){$(".error",a).removeClass("error")}function doShowEditErrors(g,b){if(!g||!b){return false}var f=false;for(var e=0;e<g.length;e++){var d=g[e];for(var a in d){var c=d[a];f=true;doShowError(b,a,e,c)}}return f}function doShowError(c,b,e,d){var a=$("[name='"+b+"']",c);if(!a.length){a=$("[name='"+b+"["+e+"]']",c)}if(!a.length){a=$("[name='"+b+"["+e+"][]']",c)}if(!a.length){a=$("[name='"+b+"["+e+"][0]'],[name='"+b+"["+e+"][1]']",c)}a.parents(".bx-form-element:first").addClass("bx-form-element-error").find(".bx-form-error > [float_info]").attr("float_info",d)};
